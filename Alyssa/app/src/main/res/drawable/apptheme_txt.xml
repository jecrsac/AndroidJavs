<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_window_focused="false" android:state_enabled="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke
                android:width="1dip"
                android:color="@color/CNTAA" />
            <gradient
                android:angle="-90"
                android:startColor="@color/CNTCC"
                android:endColor="@color/CNTCC"  />
        </shape>
    </item>


    <item android:state_window_focused="false" android:state_enabled="false" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke
                android:width="1dip"
                android:color="@color/CNT22" />
            <gradient
                android:angle="-90"
                android:startColor="@color/CNTEE"
                android:endColor="@color/CNTEE"  />
        </shape>
    </item>



    <item android:state_enabled="true" android:state_focused="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke
                android:width="1dip"
                android:color="@color/CCP01" />
            <gradient
                android:angle="-90"
                android:startColor="@color/CNTFF"
                android:endColor="@color/CNTFF"  />
        </shape>
    </item>


    <item android:state_enabled="true" android:state_activated="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke
                android:width="1dip"
                android:color="@color/CCP04" />
            <gradient
                android:angle="-90"
                android:startColor="@color/CNTEE"
                android:endColor="@color/CNTEE"  />
        </shape>
    </item>


    <item android:state_focused="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke
                android:width="1dip"
                android:color="@color/CNTFF" />
            <gradient
                android:angle="-90"
                android:startColor="@color/CNTFF"
                android:endColor="@color/CNTFF"  />
        </shape>
    </item>

</selector>


