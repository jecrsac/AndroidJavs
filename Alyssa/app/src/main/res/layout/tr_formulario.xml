<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.jace.developers.alyssa.X99999.DX.TR.TRFR">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </android.support.design.widget.AppBarLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/m18800"
        android:paddingLeft="@dimen/m18800"
        android:paddingRight="@dimen/m18800"
        android:paddingTop="@dimen/m18800"
        android:background="@color/CNTEE"
        android:layout_gravity="top"
        tools:context="com.jace.developers.alyssa.X99999.DX.AS.XDUA"
        android:orientation="vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:fillViewport="true" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/CARDTR"
                android:layout_margin="6dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardUseCompatPadding="true"
                card_view:cardCornerRadius="8dp"
                card_view:cardElevation="6dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="32dp"
                        android:textSize="7pt"
                        android:textColor="@color/CNTEE"
                        android:gravity="center"
                        android:paddingBottom="8dp"
                        android:paddingTop="8dp"
                        android:background="@color/CCP02"
                        android:text="@string/frm_10626"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_margin="@dimen/m18803">

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/LY_TRBN"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="@dimen/m18804">

                            <EditText
                                android:id="@+id/TRBN"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/f88101"
                                android:paddingLeft="@dimen/m18802"
                                android:paddingRight="@dimen/m18802"
                                android:hint="@string/frm_10620"
                                android:textColorHint="@color/CNT00"
                                android:textColor="@color/CNT00"
                                android:textStyle="normal"
                                android:textSize="@dimen/CLS03"
                                android:gravity="center_vertical"
                                android:inputType="textCapCharacters|textNoSuggestions" />

                        </android.support.design.widget.TextInputLayout>

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/LY_TRDN"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="@dimen/m18804">

                            <EditText
                                android:id="@+id/TRDN"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/f88105"
                                android:paddingTop="@dimen/m18803"
                                android:paddingLeft="@dimen/m18803"
                                android:paddingRight="@dimen/m18803"
                                android:hint="@string/frm_10621"
                                android:textColorHint="@color/CNT00"
                                android:textColor="@color/CNT00"
                                android:textStyle="normal"
                                android:gravity="top|start"
                                android:textSize="@dimen/CLS03"
                                android:inputType="textCapCharacters|textNoSuggestions|textMultiLine" />

                        </android.support.design.widget.TextInputLayout>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/CARDPG"
                android:layout_margin="6dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardUseCompatPadding="true"
                card_view:cardCornerRadius="8dp"
                card_view:cardElevation="6dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="32dp"
                        android:textSize="7pt"
                        android:textColor="@color/CNTEE"
                        android:gravity="center"
                        android:paddingBottom="8dp"
                        android:paddingTop="8dp"
                        android:background="@color/CCP02"
                        android:text="@string/frm_10627"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_margin="@dimen/m18803"
                        android:paddingTop="@dimen/m18803">

                        <CheckBox
                            android:id="@+id/PGVL"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/f88101"
                            android:paddingLeft="@dimen/m18802"
                            android:paddingRight="@dimen/m18802"
                            android:hint="@string/frm_10623"
                            android:textColor="@color/CNT00"
                            android:textStyle="normal"
                            android:textSize="@dimen/CLS03"
                            android:gravity="center_vertical" />

                        <CheckBox
                            android:id="@+id/PGPG"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/f88101"
                            android:paddingLeft="@dimen/m18802"
                            android:paddingRight="@dimen/m18802"
                            android:hint="@string/frm_10624"
                            android:textColor="@color/CNT00"
                            android:textStyle="normal"
                            android:textSize="@dimen/CLS03"
                            android:gravity="center_vertical" />

                        <CheckBox
                            android:id="@+id/PGEF"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/f88101"
                            android:paddingLeft="@dimen/m18802"
                            android:paddingRight="@dimen/m18802"
                            android:hint="@string/frm_10625"
                            android:textColor="@color/CNT00"
                            android:textStyle="normal"
                            android:textSize="@dimen/CLS03"
                            android:gravity="center_vertical" />

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <!-- BUTTON SAVE -->
            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/SAVE"
                android:layout_margin="6dp"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                card_view:cardUseCompatPadding="true"
                card_view:cardCornerRadius="8dp"
                card_view:cardElevation="6dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="end"
                    android:layout_margin="@dimen/m18803">

                    <Button
                        android:id="@+id/TRSV"
                        android:layout_width="150dp"
                        android:layout_height="@dimen/f88101"
                        android:paddingLeft="@dimen/m18802"
                        android:paddingRight="@dimen/m18802"
                        android:text="@string/crd_36105_frm"
                        android:textColor="@color/CNTFF"
                        android:textSize="@dimen/CLS03"
                        android:textAlignment="center"
                        android:gravity="center_vertical" />

                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>

    </ScrollView>

</android.support.design.widget.CoordinatorLayout>
